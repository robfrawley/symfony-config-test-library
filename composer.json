{
    "name"        : "scribe/symfony-config-test-lib",
    "license"     : "MIT",
    "type"        : "library",
    "homepage"    : "https://scr.be/config-test-library",
    "description" : "Library for testing user classes related to the Symfony Config Component.",
    "support" : {
        "email"  : "rmf@scr.be",
        "issues" : "https://symfony-config-test-library.docs.scribe.tools/issues",
        "source" : "https://symfony-config-test-library.docs.scribe.tools/git",
        "irc"    : "irc://irc.efnet.org/scribe"
    },
    "keywords"    : [
        "symfony-config-test-library",
        "scribe-config-test-library",
        "config-test-library",
        "config-test",
        "phpunit",
        "symfony",
        "symfony2",
        "symfony-framework",
        "scribe",
        "scr",
        "scribenet",
        "scribeinc",
        "scribe-software",
        "scribe-services"
    ],
    "authors" : [
        {
            "name"     : "Rob Frawley 2nd",
            "email"    : "rmf@scr.be",
            "homepage" : "https://scr.be/rmf",
            "role"     : "Project Lead"
        },
        {
            "name"     : "Matthias Noback",
            "email"    : "matthiasnoback@gmail.com",
            "homepage" : "http://php-and-symfony.matthiasnoback.nl"
        }
    ],
    "require" : {
        "php"                                  : ">=5.6.4",
        "symfony/config"                       : "~2.6,>=2.6"
    },
    "require-dev": {
        "phpunit/phpunit"                      : "~4.6",
        "sebastian/exporter"                   : "~1.2",
        "scrutinizer/ocular"                   : "dev-master",
        "satooshi/php-coveralls"               : "dev-multipleroot"
    },
    "autoload": {
        "psr-4" : {
            "Scribe\\UnitTest\\SymfonyConfig\\" : "lib"
        }
    },
    "config" : {
        "bin-dir"           : "bin",
        "preferred-install" : "auto",
        "minimum-stability" : "stable"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/scribenet/php-coveralls.git"
        }
    ]
}
